---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>in exile</title>
	<link href="http://bartulem.github.io/in exile/atom.xml" rel="self"/>
	<link href="http://bartulem.github.io/in exile"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://bartulem.github.io/in exile</id>
	<author>
		<name>Bartul Mimica</name>
		<email>mimica.bartul@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://bartulem.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://bartulem.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
